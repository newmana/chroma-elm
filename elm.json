{
    "type": "package",
    "name": "newmana/chroma-elm",
    "summary": "An Elm native version of chroma.js for color maps, color spaces and operations.",
    "license": "Apache-2.0",
    "version": "12.1.1",
    "exposed-modules": {
        "Main API": [
            "Chroma.Chroma"
        ],
        "Input": [
            "Chroma.Converter.In.Cmyk2Rgb",
            "Chroma.Converter.In.Hex2Rgb",
            "Chroma.Converter.In.Hsla2Rgb",
            "Chroma.Converter.In.Lab2Lch",
            "Chroma.Converter.In.Lab2Rgb",
            "Chroma.Converter.In.Lch2Lab"
        ],
        "Output": [
            "Chroma.Converter.Out.ToCmyk",
            "Chroma.Converter.Out.ToHex",
            "Chroma.Converter.Out.ToHsla",
            "Chroma.Converter.Out.ToLab",
            "Chroma.Converter.Out.ToLch",
            "Chroma.Converter.Out.ToRgba"
        ],
        "Colors": [
            "Chroma.Colors.Brewer",
            "Chroma.Colors.Cividis",
            "Chroma.Colors.Inferno",
            "Chroma.Colors.Magma",
            "Chroma.Colors.Parula",
            "Chroma.Colors.Plasma",
            "Chroma.Colors.Sinebow",
            "Chroma.Colors.Turbo",
            "Chroma.Colors.Viridis",
            "Chroma.Colors.W3CX11"
        ],
        "Limits": [
            "Chroma.Limits.Limits",
            "Chroma.Limits.Analyze",
            "Chroma.Limits.CkMeans",
            "Chroma.Limits.Equal",
            "Chroma.Limits.Logarithmic",
            "Chroma.Limits.Quantile"
        ],
        "Operations": [
            "Chroma.Ops.Alpha",
            "Chroma.Ops.Lightness",
            "Chroma.Ops.Luminance",
            "Chroma.Ops.Saturate"
        ],
        "Helpers": [
            "Chroma.Types",
            "Chroma.Interpolator",
            "Chroma.Scale"
        ]
    },
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "avh4/elm-color": "1.0.0 <= v < 2.0.0",
        "elm/core": "1.0.0 <= v < 2.0.0",
        "mgold/elm-nonempty-list": "4.0.2 <= v < 5.0.0",
        "rtfeldman/elm-hex": "1.0.0 <= v < 2.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.2.0 <= v < 2.0.0"
    }
}